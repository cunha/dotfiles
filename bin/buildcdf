#!/usr/bin/python3

import sys

HEIGHT_STEP = 0.00001

def buildcdf(iterator):
    result = list()

    last, cnt = next(iterator)
    if last is None:
        return [[0.0, 0.0], [0.0, 1.0]]

    val, weight = next(iterator)
    while val is not None:
        if val != last:
            assert not result or last > result[-1][0], 'input not sorted'
            result.append((last, cnt))
            cnt += weight
        else:
            cnt += weight
        last = val
        val, weight = next(iterator)

    if not result:
        result.append([0.0, 0.0])
    result.append([last, cnt])

    cdf = list()

    h = HEIGHT_STEP
    i = 0
    while i < len(result):
        x, y = result[i][0], float(result[i][1])/cnt
        while y < h:
            i += 1
            if i == len(result):
                break
            x, y = result[i][0], float(result[i][1])/cnt
        cdf.append((x, y))
        while y >= h:
            h += HEIGHT_STEP
        i += 1
    cdf.append((result[-1][0], 1.0))
    return cdf


def parse_lines(fd):
    for line in fd:
        if not line:
            yield None, None
        fields = line.split()
        weight = 1.0
        if len(fields) > 1:
            weight = float(fields[1])
        yield [float(fields[0]), weight]
    yield None, None


if __name__ == '__main__':
    dcdf = buildcdf(parse_lines(sys.stdin))
    for dx, dy in dcdf:
        sys.stdout.write('%f %f\n' % (dx, dy))
    sys.exit(0)
