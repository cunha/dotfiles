{
  "title": "Application Shortcuts",
  "rules": [
    {
      "description": "Numbered application shortcuts",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "1"
          },
          "to": [
            {
              "shell_command": "open -a /Applications/Firefox.app"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "numlock_mode",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "2"
          },
          "to": [
            {
              "shell_command": "open -a /Applications/WezTerm.app"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "numlock_mode",
              "value": 1
            }
          ]
        }
      ]
    }
  ]
}
