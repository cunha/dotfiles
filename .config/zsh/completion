BREW_ZSH_SITE_FUNCTIONS=/home/linuxbrew/.linuxbrew/share/zsh/site-functions
if [[ -d $BREW_ZSH_SITE_FUNCTIONS ]] ; then
	fpath=($BREW_ZSH_SITE_FUNCTIONS $fpath)
fi

autoload -Uz compinit ;
compinit -d $HOME/.cache/zcompdump ;

zstyle ':completion:*:*:gnuplot:*:*' file-patterns '*.plot' '*'
zstyle ':completion:*:default' list-colors ${(s.:.)LS_COLORS}
zstyle ':completion:*' menu select

source $HOME/.config/zsh/fzf/completion.zsh

